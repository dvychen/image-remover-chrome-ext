{
    "manifest_version": 2,
    "name": "Image Remover", 
    "version": "1.0",
    "description": "An extension that automatically removes all images from web pages.",
    
    "content_scripts": [
        {
            "matches": ["<all_urls>"], 
            "run_at": "document_idle",
            "all_frames": true,
            "css": ["CSS/removed.css"],
            "js": ["Scripts/remove-imgs.js"]
        }
    ]
}